<h1>Insurance Companies Admin</h1>
<br>
<h3>Show</h3>
<%= link_to 'All', :action => 'list' %> | <%= link_to 'Default', :action => 'list_default' %> | <%= link_to 'User', :action => 'list_user' %><br><br>

<%= link_to 'Back to Admin', :controller => 'admin', :action => 'menu' %>
<br><br>
<table>
  <tr>
  <% for column in InsuranceCompany.content_columns %>
    <th><%= column.human_name %></th>
  <% end %>
  </tr>
  
<% for insurance_company in @insurance_companies %>
  <tr>
  <% for column in InsuranceCompany.content_columns %>
    <td><%=h insurance_company.send(column.name) %></td>
  <% end %>
    <td><%= link_to 'Show', :action => 'show', :id => insurance_company %></td>
    <td><%= link_to 'Edit', :action => 'edit', :id => insurance_company %></td>
    <td><%= link_to 'Destroy', { :action => 'destroy', :id => insurance_company }, :confirm => 'Are you sure?', :method => :post %></td>
  </tr>
<% end %>
</table>

<%= will_paginate @insurance_companies %>

<br />

<%= link_to 'New insurance_company', :action => 'new' %>
