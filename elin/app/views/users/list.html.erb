<h1>Listing users</h1>
<small><%= link_to 'Back to menu', :controller => 'admin', :action => 'menu' %></small>
<table>
  <th>Liens</th><th>Physician Name</th><th>Clinic</th><th>E-Mail</th><th>Address</th><th>City</th><th>State</th><th>Zip</th><th>Area Code</th><th>Phone</th><th>Number</th>
<% for users in @users %>
  <tr>
	<td>(<%= Lien.count(:conditions => "user_id = " + users.id.to_s) %>)</td>
 	<td><%= users.physician_name %></td>
	<td><%= users.clinic %></td>
	<td><a href="mailto:<%= users.email %>"><%= users.email %></a></td>
	<td><%= users.address %></td>
	<td><%= users.city %></td>
	<td><%= users.state %></td>
	<td><%= users.zip %></td>
	<td><%= users.phone_area %></td>
	<td><%= users.phone_prefix %></td>
	<td><%= users.phone_suffix %></td>
    <td><%= link_to 'Show', :action => 'show', :id => users %></td>
  </tr>
<% end %>
</table>

<br />
<%= will_paginate @liens %><br><br>
<%= link_to 'New users', :action => 'new' %>
