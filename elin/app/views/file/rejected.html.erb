<h1>Rejected Documents</h1>

<table width="100%">
	<th align="left">Document ID</th><th align="left">Patient Name</th><th align="left">Submitted Date</th><th align="left">Returned Date</th><th align="left">Status</th>
<% for document in @documents %>
<% if document.updated_at > (Time.now - 90.days) %>
<tr>
	<td><%= document.id %></td>
	<td align="left"><%= document.grantee_first %> <%= document.grantee_last%></td>
	<td><%= document.sent_date%></td>
	<td><%= document.returned_date%></td>
	<td><%=document.status_condition%></td>
	

</tr>
<% end %>	
<% end %>
</table>
<br><br>
