<h2>Document Upload</h2>
<%= error_messages_for :e_file %>

<% form_for(:e_file, :url => "create", 
                      :html => { :multipart => true }) do |f| -%>
Select Document Type: <select><option>Physician Lien</option></select> 
 <p>
	<span style="color:red">PDF format only</span><br>
    <%= f.file_field :uploaded_data %>
  </p><br>
	<p>	Patient's Name<br />
		First: <%= f.text_field :grantee_first %><br />
		Last: <%= f.text_field :grantee_last %><br />
	</p>
  <p>
    <%= submit_tag 'Upload' %>
  </p>
<% end -%>

<%=link_to "Back", :action => :index%>